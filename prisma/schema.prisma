generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Setting {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  key   String
  value String
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  password String
}
